{
  "name": "node-api-bootstrap",
  "version": "1.0.0",
  "description": "Bootstrap your APIs taking advantage of Node.js, Express.js, ES6 and so on.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dgaubert/node-api-bootstrap.git"
  },
  "engines": {
    "node": "4.2.4"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "standard index.js source/**/*.js test/**/*.js script/**/*.js",
    "pretest": "npm run lint",
    "test": "mocha -r test/bootstrap -r should test/**/*-test.js",
    "test:watch": "npm run test -- -w",
    "coverage": "istanbul cover _mocha test/**/*-test.js -- -r test/bootstrap -r should",
    "start": "node ."
  },
  "keywords": [],
  "author": "Daniel Garc√≠a Aubert <@dgaubert>",
  "license": "MIT",
  "dependencies": {
    "express": "4.13.4",
    "meow": "3.7.0",
    "morgan": "^1.7.0",
    "split": "^1.0.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "istanbul": "0.4.2",
    "standard": "6.0.8",
    "mocha": "2.4.5",
    "should": "8.3.0",
    "sinon": "1.17.3"
  },
  "standard": {
    "globals": [
      "__source",
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  }
}
